      "source": [
        "# 出力したいリストを作成\n",
        "array = [\"水\", \"金\", \"地\", \"火\", \"木\", \"土\", \"天\", \"海\", \"冥\"]\n",
        "\n",
        "# カウンタ変数を用意する\n",
        "i = 1\n",
        "\n",
        "# カウンタ変数が2になるまでfor文を繰り返し実行する\n",
        "while i <= 2:\n",
        "\n",
        "    # リストarrayの要素を1つずつ順番に出力する\n",
        "    for solar_system in array:\n",
        "        print(solar_system)\n",
        "\n",
        "    # カウンタ変数の値を1増やす\n",
        "    i = i + 1"
      ],
