      "source": [
        "class Human:\n",
        "    # コンストラクタを定義する\n",
        "    def __init__(self, name, age):\n",
        "        self.name = name\n",
        "        self.age = age\n",
        "\n",
        "    # メソッドを定義する\n",
        "    def printinfo(self):\n",
        "        print(self.name, self.age)\n",
        "\n",
        "# インスタンス化する\n",
        "human = Human(\"侍太郎\", \"40\")\n",
        "\n",
        "# メソッドにアクセスして実行する\n",
        "human.printinfo()"
      ]
